  "main": "index.js",
  "scripts": {
    "webpack": "npm run repro:webpack && npm run validate:webpack",
    "repro:webpack": "webpack --config webpack.config.js",
    "validate:webpack": "node script/validate.js dist/webpack/",
    "cli": "npm run repro:cli && npm run validate:cli",
